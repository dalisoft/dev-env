{
  "name": "@dalisoft/express-dev-env",
  "version": "0.1.0",
  "description": "An modern and easy development environment for Backend development with Express",
  "module": "src/server.js",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node --experimental-modules --experimental-json-modules --experimental-wasm-modules src/server.js",
    "dev": "nodemon",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier -c .",
    "lint:analysis": "npm run lint:eslint && npm run lint:prettier",
    "test": "jest --passWithNoTests"
  },
  "engines": {
    "node": ">=12.3"
  },
  "keywords": [
    "express",
    "modern",
    "dev-env"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.3.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@dalisoft/jwt": "0.0.7",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "express": "^4.17.1",
    "express-json-validator-middleware": "^2.1.1",
    "express-jwt": "^5.3.3",
    "express-jwt-permissions": "^1.3.3",
    "file-type": "^14.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.x"
  }
}
