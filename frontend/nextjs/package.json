{
  "name": "@dalisoft/nextjs-dev-env",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@formatjs/intl-pluralrules": "^3.2.5",
    "@formatjs/intl-relativetimeformat": "^5.2.11",
    "classnames": "^2.2.6",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.mapvalues": "^4.6.0",
    "next": "^9.5.2",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.7.6",
    "react-redux": "^7.2.1",
    "redaction": "^5.1.1",
    "redux": "^4.0.5",
    "redux-batched-subscribe": "^0.1.6",
    "redux-form": "^8.3.6",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "eslint": "^7.6.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.5",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.3",
    "react-styleguidist": "^11.0.8",
    "react-test-renderer": "^16.13.1",
    "serve": "^11.3.2",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0"
  },
  "scripts": {
    "dev": "next ./src",
    "build:server": "rm -rf dist && next build ./src",
    "serve:static": "serve ./dist",
    "start": "next start ./src",
    "build:static": "npm run build:server && next export ./src -o dist",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test --watchAll",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier -c .",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "docs:dev": "styleguidist server",
    "docs": "styleguidist build",
    "predeploy": "npm run build:static",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "safari >= 9",
      "ie >= 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
