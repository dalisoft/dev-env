{
  "name": "@dalisoft/react-static-dev-env",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist artifacts tmp",
    "start": "npm run clean && react-static start",
    "stage": "NODE_ENV=development react-static build --staging",
    "build": "npm run clean && react-static build",
    "analyze": "react-static build --analyze",
    "serve": "serve dist -p 3000",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test --watchAll",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier -c .",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "docs:dev": "styleguidist server",
    "docs": "styleguidist build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "^2.2.11",
    "@formatjs/intl-relativetimeformat": "^6.0.2",
    "@reach/router": "^1.3.4",
    "classnames": "^2.2.6",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.mapvalues": "^4.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.7.6",
    "react-redux": "^7.2.0",
    "redaction": "^5.1.1",
    "redux": "^4.0.5",
    "redux-batched-subscribe": "^0.1.6",
    "redux-form": "^8.3.6"
  },
  "devDependencies": {
    "babel-eslint": "10.x",
    "eslint": "^7.4.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "react-hot-loader": "^4.12.21",
    "react-scripts": "^3.4.1",
    "react-static": "^7.4.2",
    "react-static-plugin-css-modules": "^7.2.2",
    "react-static-plugin-reach-router": "^7.4.2",
    "react-static-plugin-sass": "^7.3.0",
    "react-static-plugin-sitemap": "^7.4.2",
    "react-static-plugin-source-filesystem": "^7.4.2",
    "react-styleguidist": "^11.0.8",
    "react-test-renderer": "^16.13.1",
    "serve": "11.3.2",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0"
  }
}
